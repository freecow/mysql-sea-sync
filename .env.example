# MySQL数据库配置示例
# 复制此文件为 .env 并填入真实的配置信息

# 第一个MySQL数据库连接
MYSQL_HOST=your_mysql_host_1
MYSQL_PORT=3306
MYSQL_USER=your_mysql_user_1
MYSQL_PASSWORD=your_mysql_password_1
MYSQL_DB=your_database_name_1

# 第二个MySQL数据库连接  
MYSQL_HOST_2=your_mysql_host_2
MYSQL_PORT_2=3306
MYSQL_USER_2=your_mysql_user_2
MYSQL_PASSWORD_2=your_mysql_password_2
MYSQL_DB_2=your_database_name_2

# SeaTable服务器配置
SEATABLE_SERVER_URL=https://cloud.seatable.io

# SeaTable API Tokens - 为每个同步任务配置对应的API Token
SEATABLE_TOKEN_CONTRACT=your_contract_table_api_token
SEATABLE_TOKEN_OS=your_os_table_api_token
SEATABLE_TOKEN_PROGRESS=your_progress_table_api_token
SEATABLE_TOKEN_RD=your_rd_table_api_token
SEATABLE_TOKEN_WORKTIME=your_worktime_table_api_token
SEATABLE_TOKEN_PAYIN=your_payin_table_api_token
SEATABLE_TOKEN_PI=your_pi_table_api_token
SEATABLE_TOKEN_PROJECT=your_project_table_api_token
SEATABLE_TOKEN_GSSALES=your_gssales_table_api_token
SEATABLE_TOKEN_YZWQ=your_yzwq_table_api_token
SEATABLE_TOKEN_OUTSOURCE=your_outsource_table_api_token
SEATABLE_TOKEN_PURCHASE=your_purchase_table_api_token

# 可选：日志级别配置
LOG_LEVEL=INFO

# 可选：同步配置
DEFAULT_CHUNK_SIZE=500