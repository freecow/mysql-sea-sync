{
  "seatable": {
    "table_name": "项目进度",
    "name_column": "合同编号"
  },
  "chunk_size": 300,
  "data_mappings": [
    {
      "description": "项目进度数据",
      "sql_query": [
        "SELECT",
        "  ps.contract_code,",
        "  COALESCE(sdd_project_status.dict_label, ps.progress_status) AS progress_status,",
        "  ps.respondent,",
        "  ps.input_person,",
        "  IFNULL(ps.input_time, '') AS input_time,",
        "  IFNULL(ps.remark, '') AS remark,",
        "  IFNULL(ps.question, '') AS question,",
        "  IFNULL(ps.yjys_date, '') AS yjys_date",
        "FROM project_schedule ps",
        "LEFT JOIN sys_dict_data sdd_project_status",
        "ON ps.progress_status = sdd_project_status.dict_value AND sdd_project_status.dict_type = 'project_progress_status'"
      ],
      "field_mappings": {
        "contract_code": "合同编号",
        "progress_status": "进度状态",
        "respondent": "答复人",
        "input_person": "录入人",
        "input_time": "录入时间",
        "remark": "备注",
        "question": "遇到的问题",
        "yjys_date": "预计验收时间"
      }
    }
  ]
}
