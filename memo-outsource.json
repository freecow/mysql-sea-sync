{
  "seatable": {
    "table_name": "外包合同",
    "name_column": "项目编号"
  },
  "chunk_size": 900,
  "data_mappings": [
    {
      "description": "主表数据",
      "sql_query": [
        "SELECT",
        "  od.contract_code,",
        "  od.total_project_name,",
        "  IFNULL(od.outsourcers_name, '') AS outsourcers_name,",
        "  IFNULL(od.outsourcers_code, '') AS outsourcers_code,",
        "  IFNULL(od.outsourcers_amount, '') AS outsourcers_amount,",
        "  IFNULL(od.out_project_name, '') AS out_project_name,",
        "  IFNULL(od.outsourcers_content, '') AS outsourcers_content,",
        "  IFNULL(od.out_acceptance_report_time, '') AS out_acceptance_report_time,",
        "  IFNULL(od.out_contract_sign_time, '') AS out_contract_sign_time,",
        "  IFNULL(od.project_source, '') AS project_source,",
        "  IFNULL(od.out_document_archive, '') AS out_document_archive,",
        "  COALESCE(sdd_out_type.dict_label, od.out_type) AS out_type,",
        "  COALESCE(sdd_out_channel.dict_label, od.out_channel) AS out_channel,",
        "  COALESCE(sdd_firm_name.dict_label, od.firm_name) AS firm_name",
        "FROM outsourced_documentation od",
        "LEFT JOIN sys_dict_data sdd_firm_name ON od.firm_name = sdd_firm_name.dict_value AND sdd_firm_name.dict_type = 'firm_name'",
        "LEFT JOIN sys_dict_data sdd_out_channel ON od.out_channel = sdd_out_channel.dict_value AND sdd_out_channel.dict_type = 'out_channel'",
        "LEFT JOIN sys_dict_data sdd_out_type ON od.out_type = sdd_out_type.dict_value AND sdd_out_type.dict_type = 'out_type'"
      ],
      "field_mappings": {
        "contract_code": "项目编号",
        "total_project_name": "项目名称",
        "outsourcers_name": "外包商名称",
        "outsourcers_code": "外包合同编号",
        "outsourcers_amount": "外包合同额",
        "out_project_name": "外包项目名称",
        "outsourcers_content": "外包内容",
        "out_acceptance_report_time": "外包验收报告时间",
        "out_contract_sign_time": "外包合同签订时间",
        "out_channel": "外包渠道",
        "out_type": "外包类别",
        "project_source": "项目来源",
        "out_document_archive": "外包文档归档",
        "firm_name": "所属公司"
      }
    }
  ]
}
